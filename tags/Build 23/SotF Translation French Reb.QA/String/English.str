StringInfo
	ID "IDS_VICTORYWINDOW_CAPITALVICTORY_NAME"
	Value "Victoire par capitale"
StringInfo
	ID "IDS_VICTORYWINDOW_CAPITALVICTORY_DESCRIPTION"
