StringInfo
	ID "IDS_ABILITY_VOLLEYFIREACTIVE_DESCRIPTION"
	Value "Reprend l'utilisation normale des moteurs."
StringInfo
	ID "IDS_ABILITY_VOLATILENANITES_NAME"
