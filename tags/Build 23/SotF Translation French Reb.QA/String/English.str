StringInfo
	ID "IDS_ABILITY_SHIELDREGENERATE_DESCRIPTION"
	Value "Régénère les boucliers des cibles amies proches avec le temps."
StringInfo
	ID "IDS_ABILITY_SHIELDRESTORE_NAME"
	Value "Rétablissement de bouclier"
StringInfo
	ID "IDS_ABILITY_SHIELDRESTORE_DESCRIPTION"
	Value "Rétablit instantanément les boucliers d'une cible amie."
StringInfo
	ID "IDS_ABILITY_SHIPCRIPPLE_NAME"
	Value "Vertige"
StringInfo
	ID "IDS_ABILITY_SHIPCRIPPLE_DESCRIPTION"
	Value "Réduit les chances de toucher et la cadence de tir des ennemis affectés."
StringInfo
	ID "IDS_ABILITY_SLOWENEMYHYPERSPACE_NAME"
